{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "tsBuildInfoFile": "dist/tsconfig.lib.tsbuildinfo",
    "jsx": "preserve",
    "jsxImportSource": "vue",
    "rootDir": "src",
    "resolveJsonModule": true,
    "types": [
      "vite/client"
    ],
    "outDir": "dist"
  },
  "include": [
    "src/**/*.js",
    "src/**/*.jsx",
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.vue"
  ],
  "exclude": []
}
